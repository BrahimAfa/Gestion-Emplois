@model IEnumerable<GestionEmploi.Models.Cour>

@Styles.Render("~/Content/Filiere.css")

<div class="container">
    @Html.ActionLink("+ Add", "Create", null, null, new { @class = "btn btn-primary" })

    <br />
    <br />
    <div class="table-responsive">
        <table class="table text-center">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Code</th>
                    <th>Libelle</th>
                    <th>Filiere</th>
                    <th>Action</th>

                </tr>
            </thead>
            <tbody>


                @foreach (var cour in ViewData["list"] as IEnumerable<GestionEmploi.Models.Cour>)
                {
                    <tr>

                        <td> @cour.CourId</td>
                        <td>@cour.Libelle</td>
                        <td>@cour.libelle</td>
                        <td>@cour.Filiere.Libelle</td>


                        <td>
                            <a href="@Url.Action("Delete",new { id = niveau.NiveauId})" class="btn btn-danger">Delete</a>
                            <a href="@Url.Action("Edit",new { id = niveau.NiveauId})" class="btn btn-success">Edit</a>

                        </td>

                    </tr>




                }


            </tbody>
        </table>
</div>
